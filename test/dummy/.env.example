# Rails Pulse Dummy App Environment Variables
# Copy this file to .env and customize for your database testing

# Database Configuration
# Uncomment one of the following to switch database adapters:

# SQLite (default)
# DATABASE_ADAPTER=sqlite3

# PostgreSQL
# DATABASE_ADAPTER=postgresql
# POSTGRES_USERNAME=postgres
# POSTGRES_PASSWORD=
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432

# MySQL
# DATABASE_ADAPTER=mysql2
# MYSQL_USERNAME=root
# MYSQL_PASSWORD=
# MYSQL_HOST=localhost
# MYSQL_PORT=3306
# PARALLEL_WORKERS=1
